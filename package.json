{
  "name": "proxytron",
  "version": "1.0.2",
  "description": "Event proxying for Node EventEmitters with emphasis on reliability (100% test coverage) and readability (proxying needs to be clear). Zero dependencies, ES5+",
  "license": "MIT",
  "author": "Tim Ruffles <timrruffles@googlemail.com> (http://truffles.me.uk)",
  "engines": {
    "node": ">=0.10.0"
  },
  "scripts": {
    "test": "rm -rf coverage && istanbul cover _mocha -- *.test.js",
    "typecheck": "flow",
    "start": "mocha *.test.js --recursive --watch"
  },
  "main": "./proxytron",
  "keywords": [
    "EventEmitter",
    "proxy",
    "event proxy",
    "events",
    "event",
    "emitter",
    "emit",
    "proxying"
  ],
  "dependencies": {},
  "devDependencies": {
    "chai": "^3.5.0",
    "codecov": "^1.0.1",
    "flow-bin": "^0.22.1",
    "istanbul": "^1.0.0-alpha.2",
    "mocha": "^2.4.5"
  }
}
